<div class="main-container"
  id="ProductSection-{{ section.id }}"
  data-section-id="{{ section.id }}"
  data-section-type="product"
  data-enable-history-state="true"
  data-ajax-enabled="{{ settings.enable_ajax }}"     
>
  <div class="container-item right-side">
        <div class="map-container">
          <div class="poster">
           	<i class="horizontal">12 inch</i>
     	   	<i class="vertical">
              <i>18 inch</i>
            </i>
            <div class="layout" data-layout-type="">
              <div class="map" style="width: 900px; height: 1350px; transform: scale(0.599171);">
                	<div id="map_canvas"></div>
                	<object data="{{ "circle.svg" | asset_url }}" type="image/svg+xml" class="map-circle-svg"></object>
              </div>
              <div class="border-1" style="zoom: 1;"></div>
              <div class="border-2" style="zoom: 1;"></div>
              <div class="citymap-poster-label" style="zoom: 1;">
              <div class="border-3"></div>
                <div class="citymap-poster-left">
                  <div class="citymap-poster-title">Miami</div>
                  <div class="citymap-poster-subtitle">United States of America</div>
                  <div class="citymap-poster-tagline">25.774°N / 80.194°W</div>
              	</div>
              </div>
            </div>
           </div>

    </div>
  </div>
  
    
  {%- assign current_variant = product.selected_or_first_available_variant -%}
  {%- assign product_image_zoom_size = '1024x1024' -%}
  {%- assign product_image_scale = '2' -%}
  {%- assign enable_zoom = section.settings.enable_zoom -%}
  {%- assign compare_at_price = current_variant.compare_at_price -%}
  {%- assign price = current_variant.price -%}
  <div class="container-item left-side">
    {% capture "form_classes" -%}
        product-form product-form-{{ section.id }}{% unless section.settings.show_variant_labels %} product-form--hide-variant-labels{% endunless %}{% if section.settings.enable_payment_button and product.has_only_default_variant%} product-form--payment-button-no-variants{% endif %}
    {%- endcapture %}

    {% form 'product', product, class:form_classes, novalidate: 'novalidate', data-product-form: '' %}

        <div align="left" id="infiniteoptions-container"></div>      

        {% if section.settings.show_quantity_selector %}
            <div class="product-form__item product-form__item--quantity">
                <label for="Quantity-{{ section.id }}">{{ 'products.product.quantity' | t }}</label>
                <input type="number" id="Quantity-{{ section.id }}" name="quantity" value="1" min="1" class="product-form__input" pattern="[0-9]*" data-quantity-input>
            </div>
        {% endif %}

        <div class="product-form__error-message-wrapper product-form__error-message-wrapper--hidden{% if section.settings.enable_payment_button %} product-form__error-message-wrapper--has-payment-button{% endif %}" data-error-message-wrapper role="alert">
            <span class="visually-hidden">{{ 'general.accessibility.error' | t }} </span>
                {% include 'icon-error' %}
            <span class="product-form__error-message" data-error-message>{{ 'products.product.quantity_minimum_message' | t }}</span>
        </div>

    
    	
        <div id="accordion">
            <div class="card">
                <div class="card-header" id="headingOne">
                    <h4 class="panel-title mb-0" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true"
                    aria-controls="collapseOne">
                      <i class="fas fa-map-marker-alt"></i> <span>Choose Location</span>
                    </h4>
                </div>
            </div>
          	<div id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#accordion">
                    <div class="card-body">
                    	<div class="line-item-property__field">
<!--         					<label for="map-location">Map Location</label> -->
                            <div id="geocoder" class="geocoder"></div>
                        </div>
                    </div>
                </div>
            <div class="card">
                <div class="card-header" id="headingThree">
                    <h4 class="panel-title mb-0" data-toggle="collapse" data-target="#collapseThree"
                    aria-expanded="false" aria-controls="collapseThree">
                      <i class="fas fa-user-cog"></i> <span>Personalize</span>
                    </h4>
                </div>
            </div>
          	<div id="collapseThree" class="collapse" aria-labelledby="headingThree" data-parent="#accordion">
                    <div class="card-body">
						  <div class="form-group">
                            <label for="mapTitle">Title</label>
                            <input type="text" class="form-control" id="mapTitle">
                          </div>
						  <div class="form-group">
                            <label for="mapLabel">Label</label>
                            <input type="text" class="form-control" id="mapLabel">
                          </div>
						  <div class="form-group">
                            <label for="mapCoordinates">Coordinates</label>
                            <input type="text" class="form-control" id="mapCoordinates">
                          </div>
                    </div>
                </div>
          	<div class="card">
                <div class="card-header" id="headingTwo">
                    <h4 class="panel-title mb-0" data-toggle="collapse" data-target="#collapseTwo"
                    aria-expanded="false" aria-controls="collapseTwo">
                      <i class="fas fa-palette"></i> <span>Change Apperance and Style</span>
