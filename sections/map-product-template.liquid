                            <div class="text-row">
                              <label>Top Text</label>
                              <select id="slim-select-top"></select>                       
                              <div class="text-size-container"> 
                                <label class="text-color-label">Text Color</label>
                                <label class="text-size-label">Text Size</label>
                                <div class="color-picker-title"></div> 
                                <div class="text-size-row">
                                	<button class="text-button" id="top" data-fontsize-id="small"><span>Small</span></button>
                           		</div>
                           		<div class="text-size-row">
                                	<button class="text-button" id="top" data-fontsize-id="medium"><span>Medium</span></button>
                            	</div>
                            	<div class="text-size-row">
                                	<button class="text-button" id="top" data-fontsize-id="large"><span>Large</span></button>
                            	</div>
                              </div>
                            </div>
                            <div class="text-row">
                              <label>Middle Text</label>
                              <select id="slim-select-middle">
                              </select>
							  <div class="text-size-container"> 
                                <label class="text-color-label">Text Color</label>
                                <label class="text-size-label">Text Size</label>
                                <div class="color-picker-sub"></div> 
                                <div class="text-size-row">
                                	<button class="text-button" id="middle" data-fontsize-id="small"><span>Small</span></button>
                           		</div>
                           		<div class="text-size-row">
                                	<button class="text-button" id="middle" data-fontsize-id="medium"><span>Medium</span></button>
                            	</div>
                            	<div class="text-size-row">
                                	<button class="text-button" id="middle" data-fontsize-id="large"><span>Large</span></button>
                            	</div>
                              </div>
                            </div>
                            <div class="text-row">
                              <label>Bottom Text</label>
                              <select id="slim-select-bottom">
                              </select>
							  <div class="text-size-container"> 
                                <label class="text-color-label">Text Color</label>
                                <label class="text-size-label">Text Size</label>
                                <div class="color-picker-tag"></div> 
                                <div class="text-size-row">
                                	<button class="text-button" id="bottom" data-fontsize-id="small"><span>Small</span></button>
                           		</div>
                           		<div class="text-size-row">
                                	<button class="text-button" id="bottom" data-fontsize-id="medium"><span>Medium</span></button>
                            	</div>
                            	<div class="text-size-row">
                                	<button class="text-button" id="bottom" data-fontsize-id="large"><span>Large</span></button>
                            	</div>
                              </div>
                            </div>
                          </div>
