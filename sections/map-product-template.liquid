                      	<div class="tab-pane fade" id="pills-other" role="tabpanel" aria-labelledby="pills-other-tab">
                      		<div class="theme-option">
                                    
                                    <div class="main-poster">
                                      	<label class="text-size-label">Poster Color</label>
                                        <div class="color-picker-poster"></div> 
                                    </div>
                                    <div class="main-bottom-poster">
                                      	<label class="text-size-label">Bottom Container Color</label>
                                        <div class="color-picker-poster-label"></div> 
                                    </div>
                                    <div class="main-border-1" style="display: none;">
                                        <label class="text-size-label">Border One Color</label>
                                        <div class="color-picker-border-one"></div> 
                                    </div>
                                    <div class="main-border-2" style="display: none;">
                                      	<label class="text-size-label">Border Two Color</label>
                                        <div class="color-picker-border-two"></div> 
                                    </div>
                                    <div class="main-border-3" style="display: none;">
                                      	<label class="text-size-label">Border Three Color</label>
                                        <div class="color-picker-border-three"></div> 
                                    </div>
                              		<div class="main-circle" style="display: none;">
                                      	<label class="text-size-label">Circle Color</label>
