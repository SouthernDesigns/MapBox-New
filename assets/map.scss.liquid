  .map{
      transform-origin: 0 0;
      -webkit-transform-origin: 0 0;
  }
  .map{
  	position: absolute;
    left: 0;
    top: 0;
  }
  #map_canvas{
  	position: absolute;
    width: 100%;
    height: 100%;
    left: -1px!important;
    top: -2px!important;
  }
  .poster>div {
    .map{
    	background-size: cover;
    	background-repeat: no-repeat;
    	background-position: center;
    	overflow: hidden;
    }
    width: 30rem;
    white-space: nowrap;
  	background-size: cover;
    background-repeat: no-repeat;
    box-sizing: content-box;
    position: relative;
  }
.horizontal{
  	&:before{
        content: '';
        height: 1.125rem;
        background: rgba(51,55,68,.5);
        position: absolute;
        box-sizing: content-box;
        padding-bottom: 1px;
        top: -0.61rem;
        width: 1px;
      	left: 0;
  	}
    &:after{
        content: '';
        height: 1.125rem;
        background: rgba(51,55,68,.5);
        position: absolute;
        box-sizing: content-box;
        padding-bottom: 1px;
        top: -0.61rem;
        width: 1px;
      	right: 0;
  	}
    width: 100%;
    top: 100%;
    margin-top: 2rem;
    height: 1px;
    left: 0;
  	position: absolute;
    background: rgba(51,55,68,.5);
    font-style: normal;
    line-height: 2rem;
    text-align: center;
}
.vertical{
  	&:before{
		content: '';
        width: 1.125rem;
        background: rgba(51,55,68,.5);
        position: absolute;
        box-sizing: content-box;
        padding-right: 1px;
        left: -0.61rem;
        height: 1px;
  	}
    &:after{
		content: '';
        width: 1.125rem;
        background: rgba(51,55,68,.5);
        position: absolute;
        box-sizing: content-box;
        padding-right: 1px;
        left: -0.61rem;
        height: 1px;
      	bottom: 0;
  	}
  
  	> i {
      font-style: normal;
      position: absolute;
      top: 50%;
      width: .33333333rem;
      margin-top: -.16666667rem;
      transform: rotate(90deg);
      white-space: nowrap;
      left: 1rem;
  	}
    top: 0;
    left: 100%;
    margin-left: 2rem;
    height: 100%;
    width: 1px;
  	position: absolute;
    background: rgba(51,55,68,.5);
    font-style: normal;
    line-height: 1rem;
    text-align: center;

}

